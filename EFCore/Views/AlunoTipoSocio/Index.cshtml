@model IEnummerable<EFCore.Models.Aluno>

    <h2>Informação sobre Alunos e Taxa de Desconto</h2>

@if(!Model.Any())
{
<p>Não existem informações</p>
}else{

    <table class="table-boardered">
        <thead>
            <tr>Nome</tr>
            <tr>Taxa de desconto</tr>
            </thead>

            <tbody>

@foreach(var aluno in Model)
                {
                <tr>
                   <td> @Html.DisplayFor(m=> aluno.Nome)</td> 
                    <td> @Html.DisplayFor(m=> aluno.TipoSocio.TaxaDesconto)</td>
                </tr>
                }
            </tbody>
    </table>



}